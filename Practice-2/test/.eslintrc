{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": ["import"],
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  rules: {
    "max-len": ["error", 150],
    "require-jsdoc": "off",
    "comma-dangle": "off",
    "no-undef": "warn",

	"linebreak-style": 0
  }
}
